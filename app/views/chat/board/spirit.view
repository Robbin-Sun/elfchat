<div class="message" id="message-[id]">
    <span class="avatar" style="background-image: url(data:image/png;base64,);"></span>
    <span class="time">
        [time]
    </span>
    <span class="user">
        <span class="name" data-user-name="{{ 'Chat Spirit'|trans() }}">{{ 'Chat Spirit'|trans() }}</span>:&nbsp;
    </span>
    <span class="text">[&text]</span>
</div>